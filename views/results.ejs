  <div class="row">
<% recipes.forEach(recipe => { %>
    <div class="col s12 m7">
      <div class="card">
        <h5 class="center"><%= recipe.label %></h5>
        <div class="responsive-img center">
          <img src="<%= recipe.image %>">
        </div>
        <div class=" results card-content" >
            <form method="POST" action="/profile/favorites">
              <input class="add-recipe" type="submit" value="add to favorites">
              <p><strong>Diet Labels:</strong><%= recipe.dietLabels %></p>
              <p><strong>Health Labels:</strong><%= recipe.healthLabels %></p>
            </form>
        </div>
        <div class="card-action center">
          <a href="<%= recipe.url %>">Visit Page</a>
        </div>
      </div>
    </div>
<% }) %>
  </div>

